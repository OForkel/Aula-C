                scanf("%f", &s);
                printf("\nDigite a velocidade: ");
                scanf("%f", &v);
                delt= s -so;
                res= delt/v;
                printf("O tempo é: %.2f", res);
            }
            else if(n==2){
                float so,s,v,delt,res,t;
