    printf("Digite b: ");
    scanf("%f", &b);
    printf("Digite c: ");
    scanf("%f",&c);
    divi= 2*a;
    b1= -1*b;
    b2= b*b;
    mb=4*a*c;
    delta= b2-mb;
    if(delta <= 0 ||a==0)
    { 
        printf("Não é possivel calcular!!!");
    }
    else
    {
    raiz= sqrt(delta);
    mult1=b1-raiz;
    mult2=b1+raiz;
    r1=mult1/divi;
    r2=mult2/divi;
    printf("O resultado é:%f , %f", r1,r2);
    }
    
}
