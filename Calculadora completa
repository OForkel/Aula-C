        printf("Digite o número para transformar em fatorial: ");
        scanf("%i", &fator);
        if(fator<1){
            printf("Fatorial não definido");
        }
        else {
            while (fator>1) 
        {   
            result*=fator;
            fator--;
        }
            printf("O fatorial é: %d", result);
        }
