    scanf("%f", &a1);
    printf("Digite o lado sem angulo conhecido: ");
    scanf("%f", &l2);
    graus= a1*M_PI/180;
    sen=sin(graus);
    po= l2*sen/l1;
    res= asin(po);
    res1= res*180/M_PI;
    printf("O resultado é: %f°", res1);
}
