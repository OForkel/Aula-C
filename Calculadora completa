                v=delt/t;
                printf("A velocidade é: %.2f", v);
            }
            else if(n==3){
                float so,s,v,delt,res,t;
                printf("\nDigite o espaço inicial: ");
                scanf("%f", &so);
                printf("\nDigite o tempo: ");
                scanf("%f", &t);
                printf("\nDigite a velocidade : ");
