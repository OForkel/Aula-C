
#include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int numer;
printf("Escolha a operação:\n");

printf("1. Soma\n");
printf("2. Subtração\n" );
printf("3. Multiplicaçao\n");
printf("4. Divisão\n");
printf("5. Equação de segundo grau\n");
printf("6. Lei dos cossenos\n");
printf("7. Lei dos senos\n");
printf("8. Divisores\n");
printf("9. Fatorial\n");
printf("10. \n");
printf("Digite o numero da operação escolhida:");
scanf("%i", &numer);
if (numer== 1){ 
    float num1,num2,soma1;
    printf("Digite o primeiro número: ");
    scanf("%f",&num1);
    printf("Digite o segundo número: ");
    scanf("%f",&num2);
    soma1=num1+num2;
    printf("O resultado é %f", soma1);
}
else if (numer== 2){ 
    float num3,num4,sub1;
    printf("Digite o primeiro número: ");
    scanf("%f",&num3);
    printf("Digite o segundo número: ");
    scanf("%f",&num4);
    sub1=num3-num4;
    printf("O resultado é %f", sub1);
}
else if (numer== 3){ 
    float num5,num6,multi1;
    printf("Digite o primeiro número: ");
    scanf("%f",&num5);
    printf("Digite o segundo número: ");
    scanf("%f",&num6);
    multi1=num5*num6;
    printf("O resultado é %f", multi1);
}
else if (numer== 4){ 
    float num7,num8,div1;
    printf("Digite o primeiro número: ");
    scanf("%f",&num7);
    printf("Digite o segundo número: ");
    scanf("%f",&num8);
    div1=num7/num8;
    printf("O resultado é %f", div1);
}

else if(numer ==5)
{
    float a,b,c,b1,delta,r1,r2,divi,minhabenga,b2,mult1,mult2,raiz;
    printf("Digite a: ");
    scanf("%f", &a);
    printf("Digite b: ");
    scanf("%f", &b);
    printf("Digite c: ");
    scanf("%f",&c);
    divi= 2*a;
    b1= -1*b;
    b2= b*b;
    minhabenga=4*a*c;
    delta= b2-minhabenga;
    if(delta <= 0 ||a==0||b==0)
    { 
        printf("Não é possivel calcular!!!");
    }
    else
    {
    raiz= sqrt(delta);
    mult1=b1-raiz;
    mult2=b1+raiz;
    r1=mult1/divi;
    r2=mult2/divi;
    printf("O resultado é:%f , %f", r1,r2);
    }
    
}
else if (numer == 6)
{
    float num1,num2,quad1,quad2,lucas,coss,res,abla,ang,as;
    printf("Digite o primeiro lado:");
    scanf("%f", &num1);
    printf("Digite o segundo lado:");
    scanf("%f", &num2);
    printf("Digite o ângulo entre eles, em graus:");
    scanf("%f", &ang);
    quad1= num1*num1;
    quad2= num2*num2;
    abla= ang*M_PI/180;
    lucas= 2*num1*num2*cos(abla);
    res= sqrt(quad1+quad2-lucas);
    printf("O resultado é:%f u.m", res);
}
else if (numer == 7)
{
    int i;
    printf("1.Descobrir o angulo\n");
    printf("2.Descobrir o lado\n");
    printf("Digite o numero da operacao: ");
    scanf("%i", &i);
    if (i== 1)
{
    float l1,l2,a1,res,div1,graus,sen,po,res1;
    printf("Digite o lado com angulo conhecido: ");
    scanf("%f", &l1);
    printf("Digite o angulo conhecido, em graus: ");
    scanf("%f", &a1);
    printf("Digite o lado sem angulo conhecido: ");
    scanf("%f", &l2);
    graus= a1*M_PI/180;
    sen=sin(graus);
    po= l2*sen/l1;
    res= asin(po);
    res1= res*180/M_PI;
    printf("O resultado é: %f°", res1);
}
    else if(i==2)
    {
    float a1, l1, a2, res, graus1, graus2,div1;
    printf("Digite o angulo com lado conhecido, em graus: ");
    scanf("%f", &a1);
    printf("Digite o lado conhecido: ");
    scanf("%f", &l1);
    printf("Digite o angulo sem lado conhecido, em graus: ");
    scanf("%f", &a2);
    graus1= a1*M_PI/180;
    graus2= a2*M_PI/180;
    div1= l1/sin(graus1);
    res= sin(graus2)*div1;
    printf("O resultado é: %f u.m", res);
    }
    else{
        printf("ERROR");
    }
} 
    else if ( numer==8){
        int d;
        int x=1;
        printf("Digite um numero: ");
        scanf("%d", &d);
        printf("Os divisores de %d são : ", d);
       while(x<d+1){
       if(d%x==0) {printf("%d ",x);}
       else {}
       x++;
    }
    }
else{
        printf("ERROR");
    }
return 0;
}
