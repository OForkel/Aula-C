                    printf("\nDigite a aceleração: ");
                    scanf("%f", &a);
                    printf("\nDigite a variação do espaço: ");
                    scanf("%f", &s);
                    vo=sqrt(pow(v,2)-(2*a*s));
                    printf("A velocidade inicial é: %.2f", vo);
                }
                else if(p==3){
                    float v,vo,a,s;
                    printf("\nDigite a velocidade final: ");
