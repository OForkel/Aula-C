    float l1,l2,l3,a1,a2,a3,au,bu,cu,po,aa1,aa2,aa3,aaa1,aaa2,aaa3,m1,m2,m3;
    printf("Digite o primeiro lado do triângulo: ");
    scanf("%f", &l1);
    printf("Digite o segundo lado do triângulo: ");
    scanf("%f", &l2);
    printf("Digite o terceiro lado do triângulo: ");
    scanf("%f", &l3);
    if(l1==l2 && l2==l3){
        printf("O triângulo é equilátero com os ângulos medindo: ");
    }
    else if (l1!=l2 && l1!=l3 && l2!=l3){
        printf("O triângulo é escaleno com os ângulos medindo: ");
    }
    else{
        printf("O triângulo é isósceles com os ângulos medindo: ");
    }
    au= l1*l1;
    bu= l2*l2;
    cu= l3*l3;
    po= au+bu+cu;
    m1= -2*l1*l2;
    m2= -2*l2*l3;
    m3= -2*l1*l3;
    aa1= po/m3;
    aa2= po/m2;
    aa3= po/m3;
    aaa1= acos(aa1);
    aaa2= acos(aa2);
    aaa3= acos(aa3);
    a1=aaa1*180/M_PI;
    a2=aaa2*180/M_PI;
    a3=aaa3*180/M_PI;
    printf("A= %.2f; B= %.2f; C= %.2f;", a1,a2,a3);
}
else{
        printf("ERROR");
    }
    }
